{
  "name": "ssx",
  "version": "1.0.0",
  "repository": "ssh://git@github.com:spruceid/ssx.git",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "private": true,
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "postinstall": "npm run clean && npm run build:packages",
    "build": "lerna run build",
    "build:packages": "lerna run --ignore \"ssx-test-*\" build",
    "build:examples": "lerna run --scope \"ssx-test-*\" build",
    "clean": "lerna run clean",
    "reset": "rm -rf node_modules packages/**/dist packages/**/node_modules examples/**/dist examples/**/node_modules examples/**/.next",
    "test": "jest --config ./.jest.config.ts --runInBand --detectOpenHandles --forceExit",
    "lint": "eslint . --ignore-path .gitignore --ext .ts,.tsx,.js",
    "lint:fix": "yarn lint --fix",
    "documentation": "lerna run doc && yarn documentation:reference",
    "documentation:reference": "node scripts/build-docs.js",
    "bootstrap": "lerna bootstrap",
    "examples": "concurrently \"yarn test-dapp start\" \"yarn express-api start\"",
    "graph": "nx graph",
    "gnosis": "yarn workspace @spruceid/ssx-gnosis-extension",
    "sdk": "yarn workspace @spruceid/ssx",
    "react": "yarn workspace @spruceid/ssx-react",
    "core": "yarn workspace @spruceid/ssx-core",
    "authjs": "yarn workspace @spruceid/ssx-authjs",
    "server": "yarn workspace @spruceid/ssx-server",
    "serverless": "yarn workspace @spruceid/ssx-serverless",
    "test-dapp": "yarn workspace ssx-test-dapp",
    "react-dapp": "yarn workspace ssx-test-wagmi-react-dapp",
    "nextauth-dapp": "yarn workspace ssx-test-nextauth",
    "express-api": "yarn workspace ssx-test-express-api",
    "http-api": "yarn workspace ssx-test-http-api",
    "serverless-dynamodb-api": "yarn workspace ssx-test-serverless-dynamodb-api",
    "start:test-dapp": "yarn test-dapp start",
    "start:test-api": "yarn express-api start",
    "prepare-release": "changeset version",
    "publish-packages": "yarn reset && yarn install && yarn build && changeset publish",
    "test:e2e": "start-test 'yarn start:test-dapp' 3000 'SSX_SIGNING_KEY=test_key yarn start:test-api' 3001 'SECRET_WORDS=\"test test test test test test test test test test test junk\" synpress run'"
  },
  "devDependencies": {
    "@auth/sveltekit": "^0.2.1",
    "@babel/preset-env": "^7.18.2",
    "@changesets/cli": "^2.25.0",
    "@microsoft/api-documenter": "^7.19.4",
    "@microsoft/api-extractor": "^7.33.7",
    "@serverless/typescript": "^3.0.0",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.15.1",
    "@synthetixio/synpress": "^3.0.5",
    "@types/aws-lambda": "^8.10.71",
    "@types/cookie": "^0.5.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.5",
    "@types/jest": "^27.5.1",
    "@types/node": "^18.0.0",
    "@types/node-jose": "^1.1.10",
    "@types/nodemailer": "^6.4.7",
    "@types/on-headers": "^1.0.0",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^16.0.9",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "@webpack-cli/generators": "^2.4.2",
    "babel-core": "^6.26.0",
    "babel-jest": "^28.1.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "^6.24.1",
    "babelify": "^8.0.0",
    "browserify": "^16.2.2",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^7.3.0",
    "copy-webpack-plugin": "^11.0.0",
    "cypress": "^11.0.1",
    "declaration-bundler-webpack-plugin": "^1.0.3",
    "dotenv": "16.0.3",
    "esbuild": "^0.14.11",
    "eslint": "^8.15.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-next": "^12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eth-testing": "^1.1.0",
    "ethers": "^5.6.8",
    "gh-pages": "^4.0.0",
    "gulp": "^4.0.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^29.3.1",
    "json-schema-to-ts": "^1.5.0",
    "jsx": "^0.9.89",
    "lerna": "^5.1.2",
    "next": "^12.1.6",
    "nodemon": "^2.0.19",
    "nx": "^14.5.4",
    "prettier": "^2.6.2",
    "react": "^18.2.0",
    "react-app-rewired": "^2.2.1",
    "react-dom": "^16.6.1",
    "redis": "^4.3.1",
    "rimraf": "^3.0.2",
    "serverless": "^3.0.0",
    "serverless-esbuild": "^1.23.3",
    "serverless-offline": "^11.2.3",
    "start-server-and-test": "^1.14.0",
    "supertest": "^6.3.2",
    "svelte": "^3.55.0",
    "svelte-check": "^2.10.2",
    "ts-jest": "^28.0.8",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.8.1",
    "tsconfig-paths": "^3.9.0",
    "tsify": "^4.0.0",
    "typedoc": "^0.23.0",
    "typedoc-plugin-markdown": "^3.12.1",
    "typescript": "^4.8.4",
    "vite": "^4.1.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.9.2",
    "yarn": "^1.22.19"
  },
  "resolutions": {
    "html-webpack-plugin": "^5.0.0"
  },
  "packageManager": "yarn@3.2.0",
  "dependencies": {
    "@spruceid/create-ssx-dapp": "file:packages/create-ssx-dapp",
    "@spruceid/ssx": "file:packages/ssx-sdk",
    "@spruceid/ssx-authjs": "file:packages/ssx-authjs",
    "@spruceid/ssx-core": "file:packages/ssx-core",
    "@spruceid/ssx-gnosis-extension": "file:packages/ssx-gnosis-extension",
    "@spruceid/ssx-react": "file:packages/ssx-react",
    "@spruceid/ssx-server": "file:packages/ssx-server",
    "@spruceid/ssx-server-middleware": "file:packages/ssx-server-middleware",
    "@spruceid/ssx-serverless": "file:packages/ssx-serverless",
    "ssx-quickstart-express": "file:examples/ssx-quickstart-express",
    "ssx-test-dapp": "file:examples/ssx-test-dapp",
    "ssx-test-express-api": "file:examples/ssx-test-express-api",
    "ssx-test-gulp-dapp": "file:examples/ssx-test-gulp-dapp",
    "ssx-test-http-api": "file:examples/ssx-test-http-api",
    "ssx-test-nextauth": "file:examples/ssx-test-nextauth",
    "ssx-test-nextauth-email": "file:examples/ssx-test-nextauth-email",
    "ssx-test-onboard-react-dapp": "file:examples/ssx-test-onboard-react-dapp",
    "ssx-test-serverless-dynamodb-api": "file:examples/ssx-test-serverless-dynamodb-api",
    "ssx-test-sveltekit-auth": "file:examples/ssx-test-sveltekit-auth",
    "ssx-test-token-gated": "file:examples/ssx-test-token-gated",
    "ssx-test-wagmi-react-dapp": "file:examples/ssx-test-wagmi-react-dapp",
    "ssx-test-web3modal-v2-html": "file:examples/ssx-test-web3modal-v2-html",
    "ssx-test-web3modal-v2-react": "file:examples/ssx-test-web3modal-v2-react",
    "ssx-test-web3modal-v2-standalone": "file:examples/ssx-test-web3modal-v2-standalone"
  }
}
